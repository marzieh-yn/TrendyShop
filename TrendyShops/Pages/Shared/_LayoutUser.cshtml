                <!-- END COPYRIGHT -->
                
                <!-- BEGIN POWERED -->
                <div class="col-md-4 col-sm-4 text-right">
                    <p class="powered">Powered by: <a href="http://">Marzieh Yahyanezhad</a></p>
                </div>
                <!-- END POWERED -->
            </div>
        </div>
    </div>
